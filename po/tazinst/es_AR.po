#: installer/tazinst:48
msgid "Tazinst - SliTaz installer - Version"
msgstr "Tazinst - Instalador SliTaz - Versión"

#: installer/tazinst:49
msgid "Usage"
msgstr "Uso"

#: installer/tazinst:50
msgid "tazinst [command] [config-file|url|options]"
msgstr "tazinst [commando] [config-file|url|optiones]"

#: installer/tazinst:51
